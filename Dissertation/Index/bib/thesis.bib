@article{McClelland2002,
   author = {McClelland, J. W. and Montoya, J. P.},
   title = {Trophic Relationships and the Nitrogen Isotopic Composition of Amino Acids in Plankton},
   journal = {Ecology (Durham)},
   volume = {83},
